{
	"name": "s3-uploader-express",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"start": "ts-node src/index.ts",
		"dev": "nodemon --exec ts-node src/index.ts",
		"dev:local": "nodemon --exec ts-node src/index.local.ts",
		"build": "tsc",
		"lambda:generate": "claudia generate-serverless-express-proxy --express-module dist/index",
		"lambda:deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region ap-south-1",
		"lambda:deploy-default-profile": "claudia create --handler lambda.handler --deploy-proxy-api --region ap-south-1 --profile default",
		"lambda:deploy-custom-profile": "PROFILE=default claudia create --handler lambda.handler --deploy-proxy-api --region ap-south-1 --profile $PROFILE"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@aws-sdk/client-s3": "^3.438.0",
		"@types/morgan": "^1.9.7",
		"aws-serverless-express": "^3.4.0",
		"dotenv": "^16.3.1",
		"morgan": "^1.10.0",
		"express": "^4.18.2",
		"@types/body-parser": "^1.19.4",
		"@types/express": "^4.17.20",
		"body-parser": "^1.20.2",
		"nodemon": "^3.0.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	}
}
